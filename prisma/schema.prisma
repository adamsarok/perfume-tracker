generator client {
  provider = "prisma-client-js"
  binaryTargets = ["linux-musl-openssl-3.0.x", "windows"] 
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Perfume {
  id Int @id @default(autoincrement())
  house String
  perfume String
  rating Float
  worn PerfumeWorn[]
}

model PerfumeWorn {
  id Int @id @default(autoincrement())
  perfume Perfume @relation(fields: [perfumeId], references: [id])
  perfumeId Int
  wornOn DateTime
}