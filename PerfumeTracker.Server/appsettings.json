{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "PerfumeTrackerServer",
    "Audience": "PerfumeTrackerClient",
    "ExpirationHours": 24
  },
  "RateLimits": {
    "General": 250,
    "Auth": 50,
    "Upload": 25
  },
  "Users": {
    "AdminUserName": "admin",
    "AdminEmail": "admin@example.com",
    "DemoUserName": "demo",
    "DemoEmail": "demo@example.com",
    "InviteOnlyRegistration": true
  },
  "R2": {
    "BucketName": "test",
    "ExpirationHours": 24,
    "MaxFileSizeKb": 256
  },
  "CORS": {
    "AllowedOrigins": "http://localhost:3000;https://localhost:3000;http://192.168.1.79:3000;https://192.168.1.79:3000"
  },
  "Missions": {
    "WeeklyCreateCount": 4
  }
}
